{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "lib": [
      "dom",
      "es2017"
    ],
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/*"
      ]
    }
  }
}